[
  {
    "name": "nginx",
    "image": "${ecr_repository}:latest",
    "cpu": 128,
    "memory": 128,
    "essential": true,
    "portMappings": [
      {
        "containerPort": 80
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-stream-prefix": "revisionist-${environment}",
        "awslogs-region": "${log_region}",
        "awslogs-group": "${log_group}"
      }
    }
  }
]
